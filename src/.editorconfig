# To learn more about .editorconfig see https://aka.ms/editorconfigdocs
root = true

# All files
[*]
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.json]
indent_style = space
indent_size = 2

[*.csproj]
indent_style = space
indent_size = 2

[*.cs]
indent_style = space
indent_size = 4
